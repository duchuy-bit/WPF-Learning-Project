<Window x:Class="Learing.View.TextBlock"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Learing.View"
        mc:Ignorable="d"
        Title="TextBox" Height="450" Width="800">
    <StackPanel>
        <TextBox Text="Dev Panda" IsEnabled="True"></TextBox>
        <PasswordBox Password="123" PasswordChar="*"></PasswordBox>

        <TextBox x:Name="txbValue" Margin="0 50 0 0" TextChanged="changeText"></TextBox>
        <TextBlock x:Name="tblValue" Text="Dev Panda"></TextBlock>

        <Button Content="Click" Click="Button_Click"></Button>
    </StackPanel>
</Window>
